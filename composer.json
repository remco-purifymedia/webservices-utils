{
  "name": "pfm_webservices_nl/utils",
  "description": "Webservices Utils library with all purpose functions (array, string, entity, json)",
  "keywords": [
    "utils",
    "webservicesnl",
    "webservices.nl"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "WebservicesNl",
      "email": "tech@webservices.nl",
      "homepage": "https://webservices.nl"
    }
  ],
  "prefer-stable": true,
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist",
    "allow-plugins": {
      "phpro/grumphp": true
    }
  },
  "require": {
    "php": "*",
    "ext-mbstring": "*",
    "ddeboer/transcoder": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "phpro/grumphp": "*",
    "friendsofphp/php-cs-fixer": "*",
    "league/factory-muffin-faker": "^2.0",
    "league/factory-muffin": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\Utils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Utils\\Test\\": "test/"
    }
  }
}
